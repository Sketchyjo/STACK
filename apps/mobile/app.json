{
  "expo": {
    "name": "Stack",
    "slug": "mobile",
    "scheme": "stack",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.josephweb3.stack",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.josephweb3.stack"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "fonts": {
      "SF-Pro-Rounded-Thin": "./assets/fonts/SF-Pro-Rounded-Thin.otf",
      "SF-Pro-Rounded-Ultralight": "./assets/fonts/SF-Pro-Rounded-Ultralight.otf",
      "SF-Pro-Rounded-Light": "./assets/fonts/SF-Pro-Rounded-Light.otf",
      "SF-Pro-Rounded-Regular": "./assets/fonts/SF-Pro-Rounded-Regular.otf",
      "SF-Pro-Rounded-Medium": "./assets/fonts/SF-Pro-Rounded-Medium.otf",
      "SF-Pro-Rounded-Semibold": "./assets/fonts/SF-Pro-Rounded-Semibold.otf",
      "SF-Pro-Rounded-Bold": "./assets/fonts/SF-Pro-Rounded-Bold.otf",
      "SF-Pro-Rounded-Heavy": "./assets/fonts/SF-Pro-Rounded-Heavy.otf",
      "SF-Pro-Rounded-Black": "./assets/fonts/SF-Pro-Rounded-Black.otf"
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "4338a5ba-c9df-49e8-b9c3-c6db820323e2"
      }
    }
  }
}
