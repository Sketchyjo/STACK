    "@react-navigation/stack": "^7.1.1",
    "@stack/shared-types": "workspace:*",
    "@stack/ui": "workspace:*",
    "expo": "~54.0.10",
    "expo-constants": "~18.0.9",
    "expo-dev-client": "~6.0.12",
    "expo-font": "^14.0.8",
    "expo-linking": "~8.0.8",
